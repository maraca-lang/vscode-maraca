{
  "name": "vscode-maraca",
  "displayName": "Maraca",
  "description": "Maraca language support",
  "version": "0.3.0",
  "publisher": "jsiwhitehead",
  "engines": {
    "vscode": "^1.29.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "maraca",
        "aliases": [
          "Maraca",
          "maraca"
        ],
        "extensions": [
          ".ma"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "maraca",
        "scopeName": "source.maraca",
        "path": "./syntaxes/maraca.tmLanguage.json"
      }
    ]
  }
}
